{
    "version": 2,
    "builds": [
      { "src": "web/package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
      { "src": "/server/(.*)", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/server/(.*)", "dest": "/server/index.js" },
      { "src": "/(.*)", "dest": "/web/$1" }
    ]
  }